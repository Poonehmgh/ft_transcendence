generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  email      String   @unique
  userName   String
  intraID    String
  passHash   String
  avatar     String   @default("empty")
  badgeName  String
  chatStatus String
  friends    Int[]    @default([0])
  test       String   @default("hi")
}
